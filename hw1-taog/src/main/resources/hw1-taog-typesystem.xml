<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>typeSystemDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <types>
    <typeDescription>
      <name>BaseAnnotation</name>
      <description>BaseAnnotation who provides confidence score and record the name of the component that produced the annotation.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>confidence</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>nameProduced</name>
          <description>Name of the component that produced the annotation</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Sentence</name>
      <description/>
      <supertypeName>BaseAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokenList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>Token</elementType>
        </featureDescription>
        <featureDescription>
          <name>nGramList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>NGram</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Answer</name>
      <description/>
      <supertypeName>Sentence</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>TestElement</name>
      <description>The entire test element
</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>question</name>
          <description/>
          <rangeTypeName>Question</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answerList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>Answer</elementType>
        </featureDescription>
        <featureDescription>
          <name>precision</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Token</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>NGram</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>n</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokenList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>Token</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>Question</name>
      <description/>
      <supertypeName>Sentence</supertypeName>
    </typeDescription>
  </types>
</typeSystemDescription>
